    "def interets():\n",
    "\n",
    "    amount=int(input('Quel montant investir: '))\n",
    "\n",
    "    rate=float(input('À quel taux: '))\n",
    "\n",
    "    years=int(input(\"Sur combien d'années: \"))\n",
    "\n",
    "    resultat=(amount*(1+rate)**years)-amount\n",
    "                \n",
    "    return(resultat)\n",
    "\n",
    "interets()"
